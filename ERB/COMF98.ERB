;-------------------------------------------------
;後背位させる
;-------------------------------------------------
@COM98

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_左尻)
SIF RESULT
	RETURN 0

;ベッドがないと立位に
SIF !BEDROOM(CFLAG:PLAYER:現在位置)
	TFLAG:193 = 1
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET

LOCALS = {PLAYER}/
CSTR:TARGET:10 += LOCALS

;射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手）
TCVAR:TARGET:2 = 1
TCVAR:PLAYER:2 = 0

;強度
TCVAR:TARGET:4 = 1700
TCVAR:PLAYER:4 = 1000 + (200 * ABL:PLAYER:Ａ感覚)

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 50
DOWNBASE:気力 += 150
DOWNBASE:PLAYER:体力 += 50
DOWNBASE:PLAYER:気力 += 150

SOURCE:PLAYER:快Ｖ = 400
SOURCE:PLAYER:情愛 = 150
SOURCE:PLAYER:苦痛 = 400
SOURCE:PLAYER:露出 = 70
SOURCE:快Ｃ = 400
SOURCE:性行動 = 500
SOURCE:征服 = 1200
SOURCE:逸脱 = 500
SOURCE:反感 = 500

;上のほうで計算した汚れデータ
SOURCE:不潔 = GET_STAINCOUNT(PLAYER, 部位_膣内, TARGET) * 10 + 60
CALL 締り具合変動, PLAYER, 3, 0
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;膣内に破瓜の血の汚れを付加
SIF TALENT:PLAYER:処女
	STAIN:PLAYER:膣内 |= 汚れ_破瓜の血
;奴隷のＰ⇔調教者のＡの汚れが移動
STAIN:Ｐ |= STAIN:PLAYER:Ｖ
STAIN:Ｐ |= STAIN:PLAYER:膣内
STAIN:PLAYER:Ｖ |= STAIN:Ｐ
STAIN:PLAYER:膣内 |= STAIN:Ｐ

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:PLAYER:Ｖ経験 ++
EXP:PLAYER:Ｖ性交経験 ++

;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 4
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
ENDIF

;愛情経験
SIF !ASSIPLAY
EXP:愛情経験 += TALENT:恋慕

IF !BEDROOM(CFLAG:PLAYER:現在位置)
	STR:0 = Get Standing Doggy
ELSE
	STR:0 = Get Doggy
ENDIF

RETURN 1

RETURN 1

@CAN_COM98(ARG)
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;縄で縛られている場合は無視（縄が無い時のみ実行判定を行う）
IF !TEQUIP:縄
	SKIPDISP ARG
	IF !BEDROOM(CFLAG:PLAYER:現在位置)
		PRINTL Get Standing Doggy
	ELSE
		PRINTL Get Doggy
	ENDIF
	TFLAG:実行値 = 0
	TFLAG:実行値表示用フラグ = 0

	;すべての命令に共通の要素を考慮
	;(従順が高いと命令に従いやすいなど)
	CALL COM_ORDER

	;ABL:欲望
	IF ABL:欲望
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += ABL:欲望 * 2
		PRINT Lust
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += ABL:奉仕精神
		PRINT Servitude
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	;ABL:サドっ気
	IF ABL:サドっ気
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += ABL:サドっ気 * 4
		PRINT Sadism
		PRINTV 'LV,ABL:サドっ気,'(,ABL:サドっ気 * 4,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	IF MARK:快楽刻印
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += MARK:快楽刻印 * 2
		PRINT Pleasure Mark
		PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 2,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	;PALAM:欲情
	IF GETPALAMLV(PALAM:欲情,5)
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 2
		PRINT Desire
		PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 2,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	;恥じらい
	IF TALENT:羞恥心 > 0
		PRINT  - 
		TFLAG:実行値 -= 1
		PRINT Shy
		PRINTV '(,1,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	
	;献身的
	IF TALENT:献身的
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 6
		PRINT Devoted
		PRINTV '(,2,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	;快感に素直
	IF TALENT:快感応答 > 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 2
		PRINT Enjoys Pleasure
		PRINTV '(,2,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	;快感の否定（淫乱、娼婦で無効）
	IF TALENT:快感応答 < 0 && !TALENT:淫乱
		PRINT  - 
		TFLAG:実行値 -= 2
		PRINT Denies Pleasure
		PRINTV '(,2,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	
	;倒錯的
	IF TALENT:倒錯的
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 4
		PRINT Perverted
		PRINTV '(,4,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	
	;サド
	IF TALENT:サド
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 3
		PRINT Sadist
		PRINTV '(,6,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	
	;小悪魔
	IF TALENT:小悪魔
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 4
		PRINT Little Devil
		PRINTV '(,4,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 5
		PRINT Love
		PRINTV '(,5,')
		TFLAG:実行値表示用フラグ = 5
	ENDIF

	;親愛
	IF TALENT:恋慕 > 1 && ASSIPLAY == 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 5
		PRINT Deep Affection
		PRINTV '(,5,')
		TFLAG:実行値表示用フラグ = 5
	ENDIF

	;淫乱
	IF TALENT:淫乱 && ASSIPLAY == 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 5
		PRINT Lewd
		PRINTV '(,5,')
		TFLAG:実行値表示用フラグ = 5
	ENDIF

	;娼婦
	IF TALENT:淫乱 > 1 && ASSIPLAY == 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 5
		PRINT Whore
		PRINTV '(,5,')
		TFLAG:実行値表示用フラグ = 5
	ENDIF
	
	;STAIN関係の計算
	LOCAL = GET_STAINCOUNT(PLAYER, 部位_膣内, TARGET)
	;汚れあり
	IF LOCAL
		PRINT  - 
		TFLAG:実行値 -= LOCAL
		PRINT Dirty
		;汚臭鈍感
		IF TALENT:汚臭耐性 > 0
			PRINT , Resistant To Smell
		ELSEIF TALENT:汚臭耐性 < 0
			PRINT , Smell Sensitive
		ENDIF
		PRINTV '(,LOCAL,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;鍵を持っていると実行可能
	IF FLAG:貞操帯鍵購入フラグ == TARGET
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 100
		PRINT Chastity Belt Key
		PRINTV '(,100,')
		TFLAG:実行値表示用フラグ = 100
	ENDIF	

	;合計を表示(30以上で実行)
	PRINT  = 
	PRINTV TFLAG:実行値

	LOCAL = 30
	SIF TFLAG:実行値 < LOCAL
		PRINT  < 
	SIF TFLAG:実行値 == LOCAL
		PRINT  = 
	SIF TFLAG:実行値 > LOCAL
		PRINT  > 
	PRINT Required Value 
	PRINTV LOCAL

	WAIT
	SKIPDISP 0
	;実行できない
	SIF TFLAG:実行値 < LOCAL
		RETURN 0
ELSE
	SKIPDISP ARG
	IF !BEDROOM(CFLAG:PLAYER:現在位置)
		PRINTL Get Forced Standing Doggy
	ELSE
		PRINTL Get Forced Doggy
	ENDIF
ENDIF
RETURN 1

