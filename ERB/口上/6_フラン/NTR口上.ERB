			ENDDATA
			PRINTDATAL
				DATAFORM %CALLNAME:奴隷%は%NTR_NAME(0)%の側で入浴している。
				DATAFORM %CALLNAME:奴隷%は%NTR_NAME(0)%の視線を気にすることなく裸身を晒している。
				DATAFORM %CALLNAME:奴隷%は%NTR_NAME(0)%のすぐ隣に座っている。
			ENDDATA
		ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_体を触らせる程度)
			PRINTDATAL
				DATAFORM 「あんまり見ないで」
				DATAFORM 「じろじろ見ちゃやだ」
				DATAFORM 「・・こっち見ないで」
			ENDDATA
			PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%の視線を気にしている。
		;ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_キスする程度)
		ELSE
			PRINTDATAL
				DATAFORM 「あんまり見ないで」
				DATAFORM 「じろじろ見ちゃやだ」
				DATAFORM 「・・こっち見ないで」
			ENDDATA
			PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%の視線から逃げるため場所を変えた。
		ENDIF
	;その他
	ELSE
		IF TALENT:奴隷:公衆便所 && TALENT:奴隷:親愛
			PRINTDATAL
				DATAFORM 「次の『おしごと』・・？ や・・やだ・・やだよぉ・・」
				DATAFORM 「え・・？ また『お店』に？ やだ・・もう、あんな事、やだぁ」
				DATAFORM 「また・・また、あんなこと・・しないといけないの？」
			ENDDATA
			PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%に『仕事』の話を聞かされ泣きそうになっている。
