	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞恥心 > 0
	PRINT  - 
	TFLAG:実行値 -= 1
	PRINT Shy
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 6
	PRINT Devoted
	PRINTV '(,6,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感に素直
IF TALENT:快感応答 > 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 2
	PRINT Enjoys Pleasure
	PRINTV '(,2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
