;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;助手が処女の場合、屈服逸脱が倍化し、従順が2になる。ただし一度きり
IF TALENT:PLAYER:処女 == 1
	TIMES SOURCE:屈従 , 5.00
	TIMES SOURCE:逸脱 , 3.00
	
	CALL LOST_VIRGIN(PLAYER, TARGET)
	IF !GETBIT(CFLAG:3,4)
		;異常経験追加（一回きり）
		EXP:異常経験 += 1
		SETBIT CFLAG:異常経験,4
	ENDIF
	IF ABL:従順 < 2
		PRINTFORML In addition %ABLNAME:0% has been increased to LV2.
		ABL:従順 = 2
	ENDIF
ENDIF
EXP:ASSI:Ｖ経験 += 1
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
IF HAS_PENIS(TARGET)
	;奴隷のＰ⇔調教者のＶの汚れが移動
	STAIN:Ｐ |= STAIN:PLAYER:Ｖ
	STAIN:PLAYER:Ｖ |= STAIN:Ｐ
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
SIF HETEROSEX(TARGET,ASSI)
	EXP:(50 + HETEROSEX(TARGET,ASSI)) += 10

RETURN 1

