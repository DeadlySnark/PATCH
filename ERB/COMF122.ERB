	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT Whore
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF

;助手が処女
IF TALENT:PLAYER:処女
	PRINT  - 
	TFLAG:実行値 -= 15
	PRINTFORM %CALLNAME:ASSI% is a virgin
	PRINTV '(,15,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;合計を表示(40以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

LOCAL = 40
SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT Required Value 
PRINTV LOCAL

WAIT

;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0

;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = Let CALLNAME:TARGET Fuck CALLNAME:ASSI
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;TCVAR:0 射精対象フラグ（ビット0=TARGET 1=PLAYER 2=MASTER 3=ASSI）
;TCVAR:1 予備射精対象フラグ（ビットを使いたくない人用、CFLAG:100に翻訳されます）	（1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI）
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手）

SETBIT TCVAR:TARGET:0, 3
TCVAR:TARGET:2 = 1
;強度
TCVAR:TARGET:4 = 2000

TEQUIP:ASSI:Ｖセックス = TARGET
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 50
DOWNBASE:気力 += 150

SOURCE:快Ｃ = 400
SOURCE:性行動 = 500
SOURCE:達成 = 400
SOURCE:屈従 = 700
SOURCE:逸脱 = 500
SOURCE:反感 = 500


;-------------------------------------------------
;特殊ソース処理
