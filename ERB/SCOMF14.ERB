;-------------------------------------------------
;母乳飲み
;愛撫系派生コマンド
;-------------------------------------------------
@SCOM14
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 10
DOWNBASE:気力 += 100

SOURCE:快Ｂ = 150
SOURCE:情愛 = 150
SOURCE:苦痛 = 50
SOURCE:露出 = 30
SOURCE:不潔 = 40
SOURCE:反感 = 70
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 10
	
;調教者が［幼児退行］
IF TALENT:PLAYER:幼児／幼児退行
	TIMES SOURCE:快Ｂ , 1.20
	TIMES SOURCE:性行動 , 1.20
ENDIF

;調教者が［幼稚］
IF TALENT:PLAYER:幼稚
	TIMES SOURCE:快Ｂ , 1.20
	TIMES SOURCE:性行動 , 1.20
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ⇔調教者の口の汚れが移動
STAIN:Ｂ |= STAIN:PLAYER:口
STAIN:PLAYER:口 |= STAIN:Ｂ
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 5
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 5
ENDIF

;調教者が猫舌持ちの場合は、に嗜虐快楽経験を追加
SIF TALENT:猫舌
	EXP:嗜虐快楽経験 += 1

RETURN 1
