;-------------------------------------------------
;アナル愛撫
;愛撫系コマンド、レベル2
;-------------------------------------------------
@COM5

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_左尻)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 40
DOWNBASE:気力 += 100

SOURCE:快Ａ = 100
SOURCE:苦痛 = 100
SOURCE:屈従 = 100
SOURCE:逸脱 = 80
SOURCE:反感 = 60
CALL 締り具合変動, TARGET, 1, 1
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 100

;Aへの苦痛は先に処理しておく
CALL PAIN_CHECK_A(TARGET)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＡ⇔調教者の指の汚れが移動
STAIN:PLAYER:手 |= STAIN:Ａ
STAIN:PLAYER:手 |= STAIN:腸内
STAIN:Ａ |= STAIN:PLAYER:手
STAIN:腸内 |= STAIN:PLAYER:手
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 2
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 2
ENDIF

RETURN 1
