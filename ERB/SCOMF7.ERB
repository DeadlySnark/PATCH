;-------------------------------------------------
;挿入子宮口責め
;系コマンド、レベル
;-------------------------------------------------
@SCOM7

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_太股)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_太股)
SIF RESULT
	RETURN 0

TCVAR:116 = PLAYER
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手）

LOCALS = {TARGET}/
CSTR:PLAYER:10 += LOCALS
TCVAR:PLAYER:2 = 1
;強度
TCVAR:PLAYER:4 = 2300

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 60
DOWNBASE:気力 += 120

SOURCE:快Ｖ = 500
SOURCE:情愛 = 200
SOURCE:苦痛 = 400
SOURCE:露出 = 120
SOURCE:反感 = 500

CALL 締り具合変動, TARGET, 3, 0
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操 > 0 && !TALENT:恋慕
	SOURCE:反感 += 1000

;Vへの苦痛は先に処理しておく
CALL PAIN_CHECK_V(TARGET)

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;膣内に破瓜の血の汚れを付加
SIF TALENT:処女
	STAIN:膣内 |= 汚れ_破瓜の血
;奴隷の膣内⇔調教者のＰの汚れが移動
STAIN:膣内 |= STAIN:PLAYER:Ｐ
STAIN:PLAYER:Ｐ |= STAIN:膣内

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｖ性交経験 ++
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 4
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
ENDIF

;愛情経験
SIF !ASSIPLAY
	EXP:愛情経験 += TALENT:恋慕

RETURN 1
