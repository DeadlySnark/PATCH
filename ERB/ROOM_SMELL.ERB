@ROOM_SMELL_ROOM_MINUS(ARG,LOCAL:0,LOCAL:1,LOCAL:10)
;ARG		部屋番号
;LOCAL:10	減算値(正の値だと引かれる)
;LOCAL:11	においの強さ

LOCAL:11 = FLAG:(LOCAL:0 + ARG) % 10000000 / 100000 - LOCAL:10
SELECTCASE LOCAL:11
CASE IS < 0
	LOCAL:11 = 0
CASE IS > 99
	LOCAL:11 = 99
ENDSELECT

IF LOCAL:11 == 0
	FLAG:(LOCAL:0 + ARG) = 0
ELSE
	FLAG:(LOCAL:0 + ARG) = FLAG:(LOCAL:0 + ARG) / 10000000 * 10000000 + LOCAL:11 * 100000 + FLAG:(LOCAL:0 + ARG) % 100000
ENDIF

LOCAL:11 = FLAG:(LOCAL:1 + ARG) % 10000000 / 100000 - LOCAL:10
SELECTCASE LOCAL:11
CASE IS < 0
	LOCAL:11 = 0
CASE IS > 99
	LOCAL:11 = 99
ENDSELECT

IF LOCAL:11 == 0
	FLAG:(LOCAL:1 + ARG) = 0
ELSE
	FLAG:(LOCAL:1 + ARG) = FLAG:(LOCAL:1 + ARG) / 10000000 * 10000000 + LOCAL:11 * 100000 + FLAG:(LOCAL:1 + ARG) % 100000
ENDIF

RETURN 0


@ROOM_SMELL_BIT(LOCAL:10)
#FUNCTION
LOCAL:10 = LOCAL:10 % 100000
RETURNF LOCAL:10

@ROOM_SMELL_VALUE(LOCAL:10)
#FUNCTION
LOCAL:10 = LOCAL:10 % 10000000 / 100000
RETURNF LOCAL:10

@ROOM_SMELL_WHOSE(LOCAL:10)
#FUNCTION
LOCAL:10 = (LOCAL:10) / 10000000
RETURNF LOCAL:10

@ROOM_SMELL_WHOSE_NAME(LOCAL:10)
#FUNCTIONS
SIF ROOM_SMELL_BIT(LOCAL:10) == 0 || ROOM_SMELL_VALUE(LOCAL:10) == 0
	RETURNF "無臭"
SIF ROOM_SMELL_BIT(LOCAL:10) == 1
	RETURNF "garbage"
LOCAL:10 = LOCAL:10 / 10000000
IF LOCAL:10 < CHARANUM
	RETURNF CALLNAME:(LOCAL:10)
ELSEIF LOCAL:10 == 精子主_訪問者 || LOCAL:10 == 人物_訪問者
	RETURNF NTR_NAME(0)
ELSEIF LOCAL:10 == 精子主_村人 || LOCAL:10 == 人物_村人
	RETURNF "villager"
ELSEIF LOCAL:10 == 精子主_客 || LOCAL:10 == 人物_客
	RETURNF "customer"
ELSE
	RETURNF "ROOM_SMELL_WHOSE_NAME error" + ToSTR(LOCAL:10)
ENDIF

@ROOM_SMELL_KINDS(LOCAL:10)
;TODO: CHECK IN GAME
#FUNCTIONS
#DIM LOOP_1
#DIMS においの種類
LOCAL:10 = ROOM_SMELL_BIT(LOCAL:10)
LOCAL:11 = 0
においの種類 = %""%
IF GETBIT(FLAG:部屋のにおい_初期FLAG, 1)
	FOR LOOP_1, 0, においの種類数最大値
		IF GETBIT(LOCAL:10, LOOP_1)
			SIF LOCAL:11 != 0
				においの種類 = %においの種類% and
			SELECTCASE LOOP_1
				CASE IS == 0
					においの種類 = garbage
					RETURNF においの種類
				CASE IS == 1
					においの種類 = %においの種類% sweat
				CASE IS == 2
					においの種類 = %においの種類% body
				CASE IS == 3
					においの種類 = %においの種類% %JUICE()%
				CASE IS == 4
					においの種類 = %においの種類% %SEMEN()%
				CASE IS == 5
					においの種類 = %においの種類% food
			ENDSELECT
			LOCAL:11++
		ENDIF
	NEXT
ELSE
	IF GETBIT(LOCAL:10, においの種類B_愛液) || GETBIT(LOCAL:10, においの種類B_精液)
		IF GETBIT(LOCAL:10, においの種類B_愛液)
			においの種類 = %においの種類% %JUICE()%
			LOCAL:11++
		ENDIF
		IF GETBIT(LOCAL:10, においの種類B_精液)
			SIF LOCAL:11
				においの種類 = %においの種類% and
			においの種類 = %においの種類% %SEMEN()%
		ENDIF
	ELSE
		FOR LOOP_1, 0, においの種類数最大値
			IF GETBIT(LOCAL:10, LOOP_1)
				SIF LOCAL:11 != 0
					においの種類 = %においの種類% and
				SELECTCASE LOOP_1
					CASE IS == 0
						においの種類 = garbage
						RETURNF においの種類
					CASE IS == 1
						においの種類 = %においの種類% sweat
					CASE IS == 2
						においの種類 = %においの種類% body
					CASE IS == 5
						においの種類 = %においの種類% food
				ENDSELECT
				LOCAL:11++
			ENDIF
		NEXT
	ENDIF
ENDIF

RETURNF においの種類

;ARG で指定したキャラの一番強いにおいを取得
@ROOM_SMELL_CHARA_MOST(ARG)
#FUNCTION
#DIM LOOP_I
LOCAL:10 = 1
FOR LOOP_I, 1, 5
	IF ROOM_SMELL_VALUE(CFLAG:ARG:(キャラのにおい_初期CFLAG + LOOP_I)) > ROOM_SMELL_VALUE(CFLAG:ARG:(キャラのにおい_初期CFLAG + LOCAL:10))
		LOCAL:10 = LOOP_I
	ELSEIF ROOM_SMELL_VALUE(CFLAG:ARG:(キャラのにおい_初期CFLAG + LOOP_I)) == ROOM_SMELL_VALUE(CFLAG:ARG:(キャラのにおい_初期CFLAG + LOCAL:10))
		SIF GETBIT(ROOM_SMELL_BIT(CFLAG:ARG:(キャラのにおい_初期CFLAG + LOOP_I)), においの種類B_精液)
			LOCAL:10 = LOOP_I
	ENDIF
NEXT
RETURNF CFLAG:ARG:(キャラのにおい_初期CFLAG + LOCAL:10)

;ARG で指定したキャラのにおいを作成
@ROOM_SMELL_CHARA_SMELL_CREATE(ARG)
;ARG		キャラ番号
;LOCAL:11	においの強さ
;LOCAL:12	においの種類

;PRINTFORML {ARG}, {CFLAG:ARG:キャラのにおい_自分のにおい}
LOCAL:11 = ROOM_SMELL_VALUE(CFLAG:ARG:キャラのにおい_自分のにおい)
LOCAL:12 = ROOM_SMELL_BIT(CFLAG:ARG:キャラのにおい_自分のにおい)
;PRINTFORML {ARG}, {LOCAL:11}, {LOCAL:12}

IF CFLAG:ARG:キャラのにおい_初期CFLAG
	SETBIT LOCAL:12, においの種類B_体臭
	SIF LOCAL:11 < CFLAG:ARG:キャラのにおい_初期CFLAG
		LOCAL:11 = CFLAG:ARG:キャラのにおい_初期CFLAG
ELSE
	CLEARBIT LOCAL:12, においの種類B_体臭
ENDIF
;PRINTFORML {ARG}, {LOCAL:11}, {LOCAL:12}

IF PALAM:ARG:潤滑 > PALAMLV:3 && HAS_VAGINA(ARG)
	SETBIT LOCAL:12, においの種類B_愛液
	SIF LOCAL:11 < においの強さ_愛液_ぐっしょり
		LOCAL:11 = においの強さ_愛液_ぐっしょり
ELSEIF PALAM:ARG:潤滑 > PALAMLV:1 && HAS_VAGINA(ARG)
	SETBIT LOCAL:12, においの種類B_愛液
	SIF LOCAL:11 < においの強さ_愛液_しっとり
		LOCAL:11 = においの強さ_愛液_しっとり
ELSE
	CLEARBIT LOCAL:12, においの種類B_愛液
ENDIF
;PRINTFORML {ARG}, {LOCAL:11}, {LOCAL:12}

IF HAS_PENIS(ARG)
	IF GETBIT(STAIN:ARG:部位_Ｐ, 汚れB_精液)
		IF MAXBASE:ARG:精力 > BASE:ARG:精力
			SETBIT LOCAL:12, においの種類B_精液
			SIF LOCAL:11 < においの強さ_精液_少量
				LOCAL:11 = においの強さ_精液_少量
		ENDIF
	ENDIF
ELSE
	CLEARBIT LOCAL:12, においの種類B_精液
ENDIF
;PRINTFORML {ARG}, {LOCAL:11}, {LOCAL:12}

CFLAG:ARG:キャラのにおい_自分のにおい = ARG * 10000000 + LOCAL:11 * 100000 + LOCAL:12
;PRINTFORML {ARG}, {CFLAG:ARG:キャラのにおい_自分のにおい}

RETURN 0


;におい情報の代入
@ROOM_SMELL_CHARA_SUB(ARG,LOCAL:10,LOCAL:11,LOCAL:12)
;ARG		キャラ番号
;LOCAL:10	誰のにおいか
;LOCAL:11	においの強さ
;LOCAL:12	においの種類
;LOCAL:13	一時変数
#DIM LOOP_1
#DIM LOOP_2

SIF LOCAL:11 > 99
	LOCAL:11 = 99
SIF LOCAL:11 < 0
	LOCAL:11 = 0
SIF LOCAL:12 >= 65536
	LOCAL:12 = 65535
SIF LOCAL:12 < 0
	LOCAL:12 = 0
IF LOCAL:11 == 0 || LOCAL:12 == 0
	LOCAL:11 = 0
	LOCAL:12 = 0
ENDIF
;PRINTFORML {LOCAL:10 * 10000000 + LOCAL:11 * 100000 + LOCAL:12}
;PRINTFORML キャラへの代入：行き先%ROOM_SMELL_WHOSE_NAME(ARG * 10000000+1111111)%{ARG}, {LOCAL:10}, {LOCAL:11}, {LOCAL:12}
IF LOCAL:10 == ARG
	FOR LOOP_1, 0, においの種類数最大値
		SIF GETBIT(ROOM_SMELL_BIT(CFLAG:ARG:キャラのにおい_自分のにおい), においの種類B_ゴミ + LOOP_1)
			SETBIT LOCAL:12, LOOP_1
	NEXT
	SIF ROOM_SMELL_VALUE(CFLAG:ARG:キャラのにおい_自分のにおい) > LOCAL:11
		LOCAL:11 = ROOM_SMELL_VALUE(CFLAG:ARG:キャラのにおい_自分のにおい)
	CFLAG:ARG:キャラのにおい_自分のにおい = LOCAL:10 * 10000000 + LOCAL:11 * 100000 + LOCAL:12
ELSE
	LOCAL:13 = 0
	FOR LOOP_1, 0, 3
		IF ROOM_SMELL_WHOSE(CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOOP_1)) == LOCAL:10
			FOR LOOP_2, 0, においの種類数最大値
				SIF GETBIT(ROOM_SMELL_BIT(CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOOP_1)), においの種類B_ゴミ + LOOP_2)
					SETBIT LOCAL:12, においの種類B_ゴミ + LOOP_2
			NEXT
			SIF ROOM_SMELL_VALUE(CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOOP_1)) > LOCAL:11
				LOCAL:11 = ROOM_SMELL_VALUE(CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOOP_1))
			SIF GETBIT(ROOM_SMELL_BIT(CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOOP_1)), においの種類B_精液) && GETBIT(LOCAL:12, においの種類B_精液) && (TCVAR:ARG:102 || TCVAR:ARG:104) && (TCVAR:ARG:103 == LOCAL:10 || TCVAR:ARG:105 == LOCAL:10)
				LOCAL:11 += 5
			SIF LOCAL:11 > 99
				LOCAL:11 = 99
			CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOOP_1) = LOCAL:10 * 10000000 + LOCAL:11 * 100000 + LOCAL:12
			RETURN 0
		ELSE
			SIF ROOM_SMELL_VALUE(CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOOP_1)) < LOCAL:11
				LOCAL:13 = LOOP_1
		ENDIF
	NEXT
	CLEARBIT LOCAL:12, においの種類_ゴミ
	SIF ROOM_SMELL_VALUE(CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOCAL:13)) < LOCAL:11
		CFLAG:ARG:(キャラのにおい_他人のにおい_1 + LOCAL:13) = LOCAL:10 * 10000000 + LOCAL:11 * 100000 + LOCAL:12
ENDIF
RETURN 0

;におい情報の代入
@ROOM_SMELL_ROOM_SUB(ARG,LOCAL:10,LOCAL:11,LOCAL:12)
;ARG		部屋番号
;LOCAL:10	誰のにおいか
;LOCAL:11	においの強さ
;LOCAL:12	においの種類
;LOCAL:13	一時変数
#DIM LOOP_I

SIF ARG > 場所番号最大値
	RETURN 0
SIF LOCAL:11 > 99
	LOCAL:11 = 99
SIF LOCAL:11 < 0
	LOCAL:11 = 0
SIF LOCAL:12 >= 65536
	LOCAL:12 = 65535
SIF LOCAL:12 < 0
	LOCAL:12 = 0
LOCAL:13 = LOCAL:10 * 10000000 + LOCAL:11 * 100000 + LOCAL:12
;PRINTFORMW 代入処理：%GETPLACENAME(ARG)%, {ARG}, %CALLNAME:(LOCAL:10)%, {LOCAL:11}, {LOCAL:12}, {LOCAL:13}, {ROOM_SMELL_WHOSE(LOCAL:13)}
IF ROOM_SMELL_WHOSE(FLAG:(部屋のにおい_初期FLAG + ARG)) == ROOM_SMELL_WHOSE(LOCAL:13) && ROOM_SMELL_BIT(FLAG:(部屋のにおい_初期FLAG + ARG)) != においの種類_ゴミ
	FOR LOOP_I, 0, においの種類数最大値
		SIF GETBIT(ROOM_SMELL_BIT(LOCAL:13), においの種類_ゴミ + LOOP_I)
			SETBIT LOCAL:12, においの種類_ゴミ + LOOP_I
	NEXT
	SIF ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG + ARG)) > ROOM_SMELL_VALUE(LOCAL:13)
		LOCAL:11 = ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG + ARG))
	LOCAL:13 = LOCAL:10 * 10000000 + LOCAL:11 * 100000 + LOCAL:12
	FLAG:(部屋のにおい_初期FLAG + ARG) = LOCAL:13
ELSEIF ROOM_SMELL_WHOSE(FLAG:(部屋のにおい_初期FLAG_2 + ARG)) == ROOM_SMELL_WHOSE(LOCAL:13) && ROOM_SMELL_BIT(FLAG:(部屋のにおい_初期FLAG_2 + ARG)) != においの種類_ゴミ
	FOR LOOP_I, 0, においの種類数最大値
		SIF GETBIT(ROOM_SMELL_BIT(LOCAL:13), においの種類_ゴミ + LOOP_I)
			SETBIT LOCAL:12, においの種類_ゴミ + LOOP_I
	NEXT
	SIF ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG_2 + ARG)) > ROOM_SMELL_VALUE(LOCAL:13)
		LOCAL:11 = ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG_2 + ARG))
	LOCAL:13 = LOCAL:10 * 10000000 + LOCAL:11 * 100000 + LOCAL:12
	FLAG:(部屋のにおい_初期FLAG_2 + ARG) = LOCAL:13
ELSEIF (ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG + ARG)) < ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG_2 + ARG))) && (ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG + ARG)) < ROOM_SMELL_VALUE(LOCAL:13))
	FLAG:(部屋のにおい_初期FLAG + ARG) = LOCAL:13
ELSEIF (ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG + ARG)) >= ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG_2 + ARG))) && (ROOM_SMELL_VALUE(FLAG:(部屋のにおい_初期FLAG_2 + ARG)) < ROOM_SMELL_VALUE(LOCAL:13))
	FLAG:(部屋のにおい_初期FLAG_2 + ARG) = LOCAL:13
ENDIF
RETURN 0

;におい情報の代入(訪問者専用)
@ROOM_SMELL_VISITER_SUB(LOCAL:10, LOCAL:11, LOCAL:12)
;LOCAL:10	誰のにおいか
