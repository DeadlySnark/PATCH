;----------------------------------------------------------------------------------------
;
;部屋のにおいパッチ
;
;----------------------------------------------------------------------------------------

;起床時の処理
@ROOM_SMELL
;各キャラの初期体臭(毎日1.5倍・ペニス持ちはさらに1.5倍)
LOCAL:3 = 1
;においが消えていく頻度(高くするとすぐに換気されます)
LOCAL:4 = 60
;消臭量
LOCAL:5 = 10

SIF !FLAG:(部屋のにおい_初期FLAG)
	RETURN 0

;起床時間と現在時刻が一致する場合, 朝の処理
IF CFLAG:MASTER:312 == TIME
	CALL ROOM_SMELL_MORNING(部屋のにおい_初期FLAG,部屋のにおい_初期FLAG_2,キャラのにおい_初期CFLAG,LOCAL:3)
;一定時間ごとににおいが換気されていく処理
ELSEIF TIME - FLAG:(LOCAL:1) > LOCAL:4
	CALL ROOM_SMELL_VENTILATION(部屋のにおい_初期FLAG,部屋のにおい_初期FLAG_2,キャラのにおい_初期CFLAG,LOCAL:5)
ENDIF

;日中の処理
CALL ROOM_SMELL_DAY(部屋のにおい_初期FLAG,部屋のにおい_初期FLAG_2,キャラのにおい_初期CFLAG)

RETURN 0


;起床時(起床時間と TIME が一致した時)にだけ呼び出される関数, 初期化等
@ROOM_SMELL_MORNING(LOCAL:0,LOCAL:1,LOCAL:2,LOCAL:3)
#DIM LOOP_ROOM
#DIM LOOP_CHARA
#DIM LOOP_1

;起床時ににおい更新時刻を記録
FLAG:(LOCAL:1) = TIME

;各部屋の汚れ具合によるにおいを上書き及び初期化
FOR LOOP_ROOM,1,場所番号最大値 + 1
	SELECTCASE FLAG:(100 + LOOP_ROOM)
	;最高 〜 やや高
	CASE IS < 1000
		FLAG:(LOCAL:0 + LOOP_ROOM) = 0
		FLAG:(LOCAL:1 + LOOP_ROOM) = 0
	;中
	CASE IS < 2000
		FLAG:(LOCAL:0 + LOOP_ROOM) = 05000001
		FLAG:(LOCAL:1 + LOOP_ROOM) = 0
	;やや低
	CASE IS < 3500
		FLAG:(LOCAL:0 + LOOP_ROOM) = 01000001
		FLAG:(LOCAL:1 + LOOP_ROOM) = 0
	;低
	CASE IS < 5000
		FLAG:(LOCAL:0 + LOOP_ROOM) = 02000001
		FLAG:(LOCAL:1 + LOOP_ROOM) = 0
	;ゴミの山
	CASEELSE
		FLAG:(LOCAL:0 + LOOP_ROOM) = 04500001
		FLAG:(LOCAL:1 + LOOP_ROOM) = 0
	ENDSELECT
	;PRINTFORML 朝の部屋汚し処理：%GETPLACENAME(LOOP_ROOM)%, 部屋番号{LOOP_ROOM}, 汚れ具合{FLAG:(100+LOOP_ROOM)}, {FLAG:(LOCAL:0+LOOP_ROOM)}
NEXT

;各キャラの体臭を日毎に増し、各部屋に追加. さらに他人のにおいを初期化
FOR LOOP_CHARA,0,CHARANUM
	FOR LOOP_1, 0, 3
		CFLAG:LOOP_CHARA:(キャラのにおい_他人のにおい_1 + LOOP_1) = 0
	NEXT
	SIF HAS_PENIS(LOOP_CHARA)
		CFLAG:LOOP_CHARA:(LOCAL:2) *= 3 / 2
	LOCAL:20 = CFLAG:LOOP_CHARA:(LOCAL:2) / 2
	CFLAG:LOOP_CHARA:(LOCAL:2) += LOCAL:20
	SIF CFLAG:LOOP_CHARA:(LOCAL:2) == 0
		CFLAG:LOOP_CHARA:(LOCAL:2) = 1
	SIF CFLAG:LOOP_CHARA:(LOCAL:2) == 0
		CFLAG:LOOP_CHARA:(LOCAL:2) = LOCAL:3
	CFLAG:LOOP_CHARA:キャラのにおい_自分のにおい = 0
	;PRINTFORML 各キャラの体臭：%CALLNAME:LOOP_CHARA%, キャラ番号{LOOP_CHARA}, 体臭{CFLAG:LOOP_CHARA:(LOCAL:2)}
	SIF CFLAG:LOOP_CHARA:(LOCAL:2) > 99
		CFLAG:LOOP_CHARA:(LOCAL:2) = 99
	;PRINTFORML {CFLAG:LOOP_CHARA:現在位置}, {LOOP_CHARA}, {CFLAG:LOOP_CHARA:(LOCAL:2)}, {においの種類_体臭}
	CALL ROOM_SMELL_ROOM_SUB(CFLAG:LOOP_CHARA:現在位置, LOOP_CHARA, CFLAG:LOOP_CHARA:(LOCAL:2), においの種類_体臭)
	;LOCAL:10 = ROOM_SMELL_WHOSE(FLAG:(500+CFLAG:LOOP_CHARA:現在位置))
	;LOCAL:11 = ROOM_SMELL_VALUE(FLAG:(500+CFLAG:LOOP_CHARA:現在位置))
	;LOCAL:12 = ROOM_SMELL_BIT(FLAG:(500+CFLAG:LOOP_CHARA:現在位置))
	;PRINTFORML {CFLAG:LOOP_CHARA:現在位置}, {LOCAL:10}, {LOCAL:11}, {LOCAL:12}, {FLAG:(500+CFLAG:LOOP_CHARA:現在位置)}
	;LOCAL:10 = ROOM_SMELL_WHOSE(FLAG:(550+CFLAG:LOOP_CHARA:現在位置))
	;LOCAL:11 = ROOM_SMELL_VALUE(FLAG:(550+CFLAG:LOOP_CHARA:現在位置))
	;LOCAL:12 = ROOM_SMELL_BIT(FLAG:(550+CFLAG:LOOP_CHARA:現在位置))
	;PRINTFORML {CFLAG:LOOP_CHARA:現在位置}, {LOCAL:10}, {LOCAL:11}, {LOCAL:12}, {FLAG:(550+CFLAG:LOOP_CHARA:現在位置)}
	;PRINTL 

	;PRINTFORML {LOOP_CHARA}, {ROOM_SMELL_WHOSE_SAMEN(LOOP_CHARA)}
	;前日に中出しされ, 洗わなかったら翌日ににおう
	IF (CFLAG:LOOP_CHARA:キャラのにおい_膣内精液の主 >= 0 && CFLAG:LOOP_CHARA:キャラのにおい_膣内精液の主 < CHARANUM) || (CFLAG:LOOP_CHARA:キャラのにおい_膣内精液の主 >= 人物_村人 && CFLAG:LOOP_CHARA:キャラのにおい_膣内精液の主 <= 人物_客)
		CALL ROOM_SMELL_CHARA_SUB(LOOP_CHARA, CFLAG:LOOP_CHARA:キャラのにおい_膣内精液の主, においの強さ_精液_熟成, においの種類B_精液)
	;前日に中出しされなくとも子宮内の精液がほのかににおう
	ELSEIF ROOM_SMELL_WHOSE_SAMEN(LOOP_CHARA)
		CALL ROOM_SMELL_CHARA_SUB(LOOP_CHARA, ROOM_SMELL_WHOSE_SAMEN(LOOP_CHARA), CFLAG:LOOP_CHARA:(LOCAL:2) , においの種類B_精液)
	ENDIF
	SIF ROOM_SMELL_BIT(ROOM_SMELL_CHARA_MOST(LOOP_CHARA)) != においの種類B_体臭
		CALL ROOM_SMELL_ROOM_SUB(CFLAG:LOOP_CHARA:現在位置, ROOM_SMELL_WHOSE(ROOM_SMELL_CHARA_MOST(LOOP_CHARA)), ROOM_SMELL_VALUE(ROOM_SMELL_CHARA_MOST(LOOP_CHARA)), ROOM_SMELL_BIT(ROOM_SMELL_CHARA_MOST(LOOP_CHARA)))
	CFLAG:LOOP_CHARA:キャラのにおい_膣内精液の主 = -1
NEXT
;訪問者の体臭を初期化
FLAG:訪問者の他人のにおい = 人物_訪問者 * 10000000 + においの強さ_訪問者の体臭 * 100000 + においの種類_体臭

RETURN 0



;日中の処理, 何度も呼び出される関数なので扱いに注意
@ROOM_SMELL_DAY(LOCAL:0,LOCAL:1,LOCAL:2)
;LOCAL:10	一時変数
;LOCAL:11	一時変数
;LOCAL:12	一時変数
;LOCAL:20	一時変数

#DIM LOOP_ROOM
#DIM LOOP_CHARA
#DIM LOOP_1
#DIM LOOP_2
#DIM ペニスのにおい
#DIM 受け側
#DIM 攻め側

;風呂に入るとにおいが消える
FOR LOOP_CHARA, 0, CHARANUM
	IF BATHROOM(CFLAG:LOOP_CHARA:現在位置)
		CFLAG:LOOP_CHARA:キャラのにおい_初期CFLAG = 1
		CFLAG:LOOP_CHARA:キャラのにおい_自分のにおい = 0
		CFLAG:LOOP_CHARA:キャラのにおい_膣内精液の主 = -1
		FOR LOOP_1, 0, 3
			CFLAG:LOOP_CHARA:(キャラのにおい_他人のにおい_1 + LOOP_1) = 0
		NEXT
	ENDIF
NEXT

;各キャラに対して適用する処理
FOR LOOP_CHARA, 0, CHARANUM
	;PRINTFORML {LOOP_CHARA}, {CFLAG:LOOP_CHARA:キャラのにおい_自分のにおい}
	;各キャラで自分のにおいを作成
	CALL ROOM_SMELL_CHARA_SMELL_CREATE(LOOP_CHARA)
	;PRINTFORML {LOOP_CHARA}, {CFLAG:LOOP_CHARA:キャラのにおい_自分のにおい}
	
	;カウンターセクハラ処理
	;抱擁など体をくっつける系はにおいをつける
	IF (TCVAR:LOOP_CHARA >= CNT_キス && TCVAR:LOOP_CHARA <= CNT_対面抱擁) || TCVAR:LOOP_CHARA == CNT_胸を揉む || (TCVAR:LOOP_CHARA >= CNT_胸愛撫 && TCVAR:LOOP_CHARA <= CNT_ディープキス) || (TCVAR:LOOP_CHARA >= CNT_ぱふぱふ && TCVAR:LOOP_CHARA <= CNT_乳合わせ)
		CALL ROOM_SMELL_CHARA_SUB(人物_あなた, LOOP_CHARA, ROOM_SMELL_VALUE(CFLAG:LOOP_CHARA:キャラのにおい_自分のにおい), ROOM_SMELL_BIT(CFLAG:LOOP_CHARA:キャラのにおい_自分のにおい))
		CALL ROOM_SMELL_CHARA_SUB(LOOP_CHARA, 人物_あなた, ROOM_SMELL_VALUE(CFLAG:人物_あなた:キャラのにおい_自分のにおい), ROOM_SMELL_BIT(CFLAG:人物_あなた:キャラのにおい_自分のにおい))
	;股間に触れたりするものもにおいをつける(においの移り先があなた→住人)
	ELSEIF TCVAR:LOOP_CHARA == CNT_股間をまさぐる || (TCVAR:LOOP_CHARA >= CNT_手淫 && TCVAR:LOOP_CHARA <= CNT_指挿入)
		LOCAL:11 = 0
		LOCAL:12 = 0
		IF PALAM:人物_あなた:潤滑 > PALAMLV:3 && HAS_VAGINA(人物_あなた)
			SETBIT LOCAL:12, においの種類B_愛液
			LOCAL:11 = においの強さ_愛液_ぐっしょり
		ELSEIF PALAM:人物_あなた:潤滑 > PALAMLV:1 && HAS_VAGINA(人物_あなた)
			SETBIT LOCAL:12, においの種類B_愛液
			LOCAL:11 = においの強さ_愛液_しっとり
		ENDIF
		IF HAS_PENIS(人物_あなた)
			IF GETBIT(STAIN:人物_あなた:部位_Ｐ, 汚れB_精液)
				IF MAXBASE:人物_あなた:精力 > BASE:人物_あなた:精力
					SETBIT LOCAL:12, においの種類B_精液
					LOCAL:11 = においの強さ_精液_少量
				ENDIF
			ENDIF
		ENDIF
		CALL ROOM_SMELL_CHARA_SUB(LOOP_CHARA, 人物_あなた, LOCAL:11, LOCAL:12)
	;股間に触れたりするものもにおいをつける(においの移り先が住人→あなた)
	ELSEIF (TCVAR:LOOP_CHARA >= CNT_クンニ強制 && TCVAR:LOOP_CHARA <= CNT_アナル奉仕) || TCVAR:LOOP_CHARA == CNT_タワシ洗い
		LOCAL:11 = 0
		LOCAL:12 = 0
		IF PALAM:LOOP_CHARA:潤滑 > PALAMLV:3 && HAS_VAGINA(LOOP_CHARA)
			SETBIT LOCAL:12, においの種類B_愛液
			LOCAL:11 = においの強さ_愛液_ぐっしょり
		ELSEIF PALAM:LOOP_CHARA:潤滑 > PALAMLV:1 && HAS_VAGINA(LOOP_CHARA)
			SETBIT LOCAL:12, においの種類B_愛液
			LOCAL:11 = においの強さ_愛液_しっとり
		ENDIF
		IF HAS_PENIS(LOOP_CHARA)
			IF GETBIT(STAIN:LOOP_CHARA:部位_Ｐ, 汚れB_精液)
				IF MAXBASE:LOOP_CHARA:精力 > BASE:LOOP_CHARA:精力
					SETBIT LOCAL:12, においの種類B_精液
					LOCAL:11 = においの強さ_精液_少量
				ENDIF
			ENDIF
		ENDIF
		CALL ROOM_SMELL_CHARA_SUB(人物_あなた, LOOP_CHARA, LOCAL:11, LOCAL:12)
	;セックス系(お互いににおいが移る)
	ELSEIF (TCVAR:LOOP_CHARA >= CNT_正常位挿入される && TCVAR:LOOP_CHARA <= CNT_背面座位A挿入される) || TCVAR:LOOP_CHARA == CNT_騎乗位逆レイプ
		CALL ROOM_SMELL_CHARA_SUB(人物_あなた, LOOP_CHARA, ROOM_SMELL_VALUE(CFLAG:LOOP_CHARA:キャラのにおい_自分のにおい), ROOM_SMELL_BIT(CFLAG:LOOP_CHARA:キャラのにおい_自分のにおい))
		CALL ROOM_SMELL_CHARA_SUB(LOOP_CHARA, 人物_あなた, ROOM_SMELL_VALUE(CFLAG:人物_あなた:キャラのにおい_自分のにおい), ROOM_SMELL_BIT(CFLAG:人物_あなた:キャラのにおい_自分のにおい))
		
		LOCAL:11 = 0
		LOCAL:12 = 0
		IF PALAM:人物_あなた:潤滑 > PALAMLV:3 && HAS_VAGINA(人物_あなた)
			SETBIT LOCAL:12, においの種類B_愛液
			LOCAL:11 = においの強さ_愛液_ぐっしょり
		ELSEIF PALAM:人物_あなた:潤滑 > PALAMLV:1 && HAS_VAGINA(人物_あなた)
			SETBIT LOCAL:12, においの種類B_愛液
			LOCAL:11 = においの強さ_愛液_しっとり
		ENDIF
		IF HAS_PENIS(人物_あなた)
			IF GETBIT(STAIN:人物_あなた:部位_Ｐ, 汚れB_精液)
				IF MAXBASE:人物_あなた:精力 > BASE:人物_あなた:精力
					SETBIT LOCAL:12, においの種類B_精液
					SIF LOCAL:11 < においの強さ_精液_少量
						LOCAL:11 = においの強さ_精液_少量
				ENDIF
			ENDIF
		ENDIF
		CALL ROOM_SMELL_CHARA_SUB(LOOP_CHARA, 人物_あなた, LOCAL:11, LOCAL:12)
		
		LOCAL:11 = 0
		LOCAL:12 = 0
		IF PALAM:LOOP_CHARA:潤滑 > PALAMLV:3 && HAS_VAGINA(LOOP_CHARA)
			SETBIT LOCAL:12, においの種類B_愛液
			LOCAL:11 = においの強さ_愛液_ぐっしょり
		ELSEIF PALAM:LOOP_CHARA:潤滑 > PALAMLV:1 && HAS_VAGINA(LOOP_CHARA)
			SETBIT LOCAL:12, においの種類B_愛液
			LOCAL:11 = においの強さ_愛液_しっとり
		ENDIF
		IF HAS_PENIS(LOOP_CHARA)
			IF GETBIT(STAIN:LOOP_CHARA:部位_Ｐ, 汚れB_精液)
				IF MAXBASE:LOOP_CHARA:精力 > BASE:LOOP_CHARA:精力
					SETBIT LOCAL:12, においの種類B_精液
					SIF LOCAL:11 < においの強さ_精液_少量
						LOCAL:11 = においの強さ_精液_少量
				ENDIF
			ENDIF
		ENDIF
		CALL ROOM_SMELL_CHARA_SUB(人物_あなた, LOOP_CHARA, LOCAL:11, LOCAL:12)
	ENDIF
	
	;住人同士交流処理　処理は上に準拠
	LOCAL:20 = CFLAG:LOOP_CHARA:住人同士イベント内容 / 10
	IF LOCAL:20
		IF CFLAG:LOOP_CHARA:住人同士イベント内容 % 10 == 0
			受け側 = LOOP_CHARA
			攻め側 = CFLAG:LOOP_CHARA:住人同士イベント相手
		ELSE
			受け側 = CFLAG:LOOP_CHARA:住人同士イベント相手
			攻め側 = LOOP_CHARA
		ENDIF
		IF LOCAL:20 == 住人同士行為_スキンシップ || (LOCAL:20 >= 住人同士行為_抱きつく && LOCAL:20 <= 住人同士行為_胸愛撫)
			CALL ROOM_SMELL_CHARA_SUB(受け側, 攻め側, ROOM_SMELL_VALUE(CFLAG:攻め側:キャラのにおい_自分のにおい), ROOM_SMELL_BIT(CFLAG:攻め側:キャラのにおい_自分のにおい))
			CALL ROOM_SMELL_CHARA_SUB(攻め側, 受け側, ROOM_SMELL_VALUE(CFLAG:受け側:キャラのにおい_自分のにおい), ROOM_SMELL_BIT(CFLAG:受け側:キャラのにおい_自分のにおい))
		;股間に触れたりするものもにおいをつける(においの移り先が受け側→攻め側)
		ELSEIF LOCAL:20 >= 住人同士行為_アナル愛撫 && LOCAL:20 <= 住人同士行為_指挿れ
			LOCAL:11 = 0
			LOCAL:12 = 0
			IF PALAM:受け側:潤滑 > PALAMLV:3 && HAS_VAGINA(受け側)
				SETBIT LOCAL:12, においの種類B_愛液
				LOCAL:11 = においの強さ_愛液_ぐっしょり
			ELSEIF PALAM:人物_あなた:潤滑 > PALAMLV:1 && HAS_VAGINA(受け側)
				SETBIT LOCAL:12, においの種類B_愛液
				LOCAL:11 = においの強さ_愛液_しっとり
			ENDIF
			IF HAS_PENIS(受け側)
				IF GETBIT(STAIN:受け側:部位_Ｐ, 汚れB_精液)
					IF MAXBASE:受け側:精力 > BASE:受け側:精力
						SETBIT LOCAL:12, においの種類B_精液
						LOCAL:11 = においの強さ_精液_少量
					ENDIF
				ENDIF
			ENDIF
			CALL ROOM_SMELL_CHARA_SUB(攻め側, 受け側, LOCAL:11, LOCAL:12)
		;セックス系(お互いににおいが移る)
		ELSEIF LOCAL:20 >= 住人同士行為_Ｖ性交Ｎ && LOCAL:20 <= 住人同士行為_Ａ後戯Ｃ
			CALL ROOM_SMELL_CHARA_SUB(受け側, 攻め側, ROOM_SMELL_VALUE(CFLAG:攻め側:キャラのにおい_自分のにおい), ROOM_SMELL_BIT(CFLAG:攻め側:キャラのにおい_自分のにおい))
			CALL ROOM_SMELL_CHARA_SUB(攻め側, 受け側, ROOM_SMELL_VALUE(CFLAG:受け側:キャラのにおい_自分のにおい), ROOM_SMELL_BIT(CFLAG:受け側:キャラのにおい_自分のにおい))
			
			LOCAL:11 = 0
			LOCAL:12 = 0
			IF PALAM:受け側:潤滑 > PALAMLV:3 && HAS_VAGINA(受け側)
				SETBIT LOCAL:12, においの種類B_愛液
				LOCAL:11 = においの強さ_愛液_ぐっしょり
			ELSEIF PALAM:人物_あなた:潤滑 > PALAMLV:1 && HAS_VAGINA(受け側)
				SETBIT LOCAL:12, においの種類B_愛液
				LOCAL:11 = においの強さ_愛液_しっとり
			ENDIF
			IF HAS_PENIS(受け側)
				IF GETBIT(STAIN:受け側:部位_Ｐ, 汚れB_精液)
					IF MAXBASE:受け側:精力 > BASE:受け側:精力
						SETBIT LOCAL:12, においの種類B_精液
						SIF LOCAL:11 < においの強さ_精液_少量
							LOCAL:11 = においの強さ_精液_少量
					ENDIF
				ENDIF
			ENDIF
			CALL ROOM_SMELL_CHARA_SUB(攻め側, 受け側, LOCAL:11, LOCAL:12)
			
			LOCAL:11 = 0
			LOCAL:12 = 0
			IF PALAM:攻め側:潤滑 > PALAMLV:3 && HAS_VAGINA(攻め側)
				SETBIT LOCAL:12, においの種類B_愛液
				LOCAL:11 = においの強さ_愛液_ぐっしょり
			ELSEIF PALAM:攻め側:潤滑 > PALAMLV:1 && HAS_VAGINA(攻め側)
				SETBIT LOCAL:12, においの種類B_愛液
				LOCAL:11 = においの強さ_愛液_しっとり
			ENDIF
			IF HAS_PENIS(攻め側)
				IF GETBIT(STAIN:攻め側:部位_Ｐ, 汚れB_精液)
					IF MAXBASE:攻め側:精力 > BASE:攻め側:精力
						SETBIT LOCAL:12, においの種類B_精液
						SIF LOCAL:11 < においの強さ_精液_少量
							LOCAL:11 = においの強さ_精液_少量
					ENDIF
				ENDIF
			ENDIF
			CALL ROOM_SMELL_CHARA_SUB(受け側, 攻め側, LOCAL:11, LOCAL:12)
		ENDIF
	ENDIF
	
	;射精処理
	;射精(中出し)された側で処理し, 強い精液のにおいが植えつけられる
	FOR LOOP_1, 0, 2
		;PRINTFORML {LOOP_CHARA}, {TCVAR:LOOP_CHARA:(102 + LOOP_1 * 2)}, {TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2)}
		IF TCVAR:LOOP_CHARA:(102 + LOOP_1 * 2)
			IF TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2) >= CHARANUM
				IF TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2) == 精子主_村人 || TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2) == 人物_村人
					LOCAL:10 = 人物_村人
				ELSEIF TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2) == 精子主_訪問者 || TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2) == 人物_訪問者
					LOCAL:10 = 人物_訪問者
				ELSEIF TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2) == 精子主_客 || TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2) == 人物_客
					LOCAL:10 = 人物_客
				ENDIF
			ELSE
				LOCAL:10 = TCVAR:LOOP_CHARA:(103 + LOOP_1 * 2)
			ENDIF
			;PRINTFORMW {LOOP_CHARA}, {LOCAL:10}
