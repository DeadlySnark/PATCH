;汚い部屋はゴミの臭いが再発生
FOR LOOP_ROOM, 1, 場所番号最大値 + 1
	SELECTCASE FLAG:(100 + LOOP_ROOM)
	;中以下はゴミの臭いが発生しない
	CASE IS < 2000
