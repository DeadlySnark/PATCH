;-------------------------------------------------------------------------------
;調教ターゲットを選択する。
;部屋内における各キャラの馴れ合い強度についても設定。
;@param ARG:0 未使用
@INFO_SetTarget(ARG:0)
	;ターゲットを初期化
	VARSET TARGET
	;CFLAG:館内地位(310)でソート
	CALL SORT_CFLAG(310)
	; LOCAL 0:ループ用、1:キャラ番号、2:ターゲット数、3:起きてるキャラの最小馴れ合い強度
	VARSET LOCAL
	LOCAL:3 = 99
	TCVAR:MASTER:馴れ合い強度=0
	;現在位置が同じキャラを取得し、ターゲット候補一覧に追加
	FOR LOCAL:0,1,CHARANUM
		;先ほど取得した館内地位順でキャラ番号を取得
		LOCAL:1 = RESULT:(LOCAL:0)
		IF CFLAG:(LOCAL:1):現在位置 == CFLAG:MASTER:現在位置
			;同じ位置にいるのなら、(寝ていたとしても)ターゲット候補に追加
			LOCAL:2++
			TARGET:(LOCAL:2) = LOCAL:1
			;ターゲットが未定だったら、とりあえず仮ターゲットとする
			IF TARGET:0 == 0
				TARGET:0 = LOCAL:1
			ENDIF
			;前回のターゲットがいるなら、(寝ていたとしても)それを最優先でターゲットにする。
			IF LOCAL:1 == TFLAG:現在のTARGET
				TARGET:0 = LOCAL:1
			ENDIF
			;馴れ合い強度は、同室中の上位キャラの馴れ合い強度以下に制限される。偉い人の前ではうふふできない。
			TCVAR:(LOCAL:1):馴れ合い強度 = MIN(CFLAG:(LOCAL:1):馴れ合い強度度,LOCAL:3)
			IF !CFLAG:(LOCAL:1):睡眠
				;起きてるキャラの最小馴れ合い強度を取得
				LOCAL:3 = MIN(CFLAG:(LOCAL:1):馴れ合い強度度,LOCAL:3)
				TCVAR:MASTER:馴れ合い強度 = MAX(CFLAG:(LOCAL:1):馴れ合い強度度,0)
			ENDIF
		ENDIF
		;DEBUGPRINTFORML %CALLNAME:(LOCAL:1)+"の馴れ合い強度:"%{TCVAR:(LOCAL:1):馴れ合い強度}
	NEXT
	;DEBUGPRINTFORML %CALLNAME:MASTER+"の馴れ合い強度:"%{TCVAR:MASTER:馴れ合い強度}
	;現在のターゲットを更新
	TFLAG:現在のTARGET = TARGET:0
RETURN 0

;-------------------------------------------------------------------------------
;うふふモードの切り替えなど
;とりあえず関数に切り出しただけの状態なんでさらにリファクタリング必要
;@param ARG:0 未使用
@INFO_SetTrainMode(ARG:0)
	#DIM 奴隷
	#DIM 同室人数
	#DIM POS_NOW		; MASTERの現在位置
	#DIM LOOP_CHR
	#DIM 告白判定値

	奴隷 = TARGET:0
	POS_NOW = CFLAG:MASTER:現在位置
	IF 奴隷 && !CFLAG:奴隷:睡眠
		;MASTERの位置から見て他の部屋にだれかいるかどうか
		同室人数 = 0
		FOR LOOP_CHR,1,CHARANUM
			SIF CFLAG:LOOP_CHR:現在位置 == POS_NOW
				CONTINUE
			SIF CAN_MOVE(POS_NOW, CFLAG:LOOP_CHR:現在位置) == 2
				同室人数 ++
		NEXT
		IF CFLAG:奴隷:うふふ == 1
			;うふふ(1)時
			TFLAG:COMABLE管理 = 2
		ELSEIF CFLAG:奴隷:うふふ == 2
			;押し倒し(2)時
			TFLAG:COMABLE管理 = 3
		ELSEIF TCVAR:MASTER:馴れ合い強度 > 2 && !同室人数 && BASE:奴隷:満足 < MAXBASE:奴隷:満足 && !OPENPLACE(POS_NOW) && POS_NOW != FLAG:訪問者の現在位置 && !TALENT:NTR && !TALENT:TARGET:公衆便所 && CHK_350_STAIN_SEMEN(奴隷)
			;あなたの馴れ合い強度が3以上で、対象が満足状態でなく、密室で、訪問者がいなく、NTRでも公衆便所でもない場合は押し倒される
			;オープンスペース（正門、広間、庭、二階踊り場、廊下）では押し倒されない
			;精液汚れによる押し倒し可否で失敗すると押し倒されない
			IF TFLAG:COMABLE管理 != 3
				PRINTL
				PRINTFORMW %CALLNAME:MASTER% has been pushed down!
				CALL KOJO_EVENT(12, TARGET)
			ENDIF
			FOR LOOP_CHR,1,CHARANUM
				CFLAG:(TARGET:LOOP_CHR):うふふ = 2
			NEXT
			CFLAG:MASTER:うふふ = 2
			TFLAG:COMABLE管理 = 3
		ELSEIF GET_TARGETNUM() < 2 && TCVAR:MASTER:130 && !GETBIT(CFLAG:1 , 0) && !CFLAG:睡眠 && BASE:MASTER:気力 > 0
			TFLAG:COMABLE管理 = 1
			告白判定値 = 0
			告白判定値 += GET_MARK_LEVEL(奴隷,刻印番号_屈服刻印,MASTER) * 3 ;あなたが付けた屈服刻印のLV×3
			告白判定値 += GET_MARK_LEVEL(奴隷,刻印番号_快楽刻印,MASTER) * 3 ;あなたが付けた快楽刻印のLV×3
			告白判定値 += MIN(ABL:親密 * 10, 50)                            ;親密×10、最大50
			告白判定値 += MIN(ABL:欲望 * 10, 50)                            ;欲望×10、最大50
			告白判定値 += GETPALAMLV(PALAM:欲情, 5) * 5                     ;好意レベル×5
			告白判定値 += GETPALAMLV(PALAM:好意, 5) * 5                     ;好意レベル×5
			告白判定値 += BASE:ムード / 50                                  ;ムードによる増加:0〜30
			告白判定値 += (1000 - BASE:理性) / 30                           ;理性による増加:0〜33
			;NTR時告白してこない
			;;あなたが肉便器の時は、その限りではない（恋情ではないから）
			IF TALENT:NTR && !TALENT:MASTER:肉便器
				;ToDo 判定に好意度を含めるか？
				;屈服度の方が高いとき告白できない。
			ELSEIF CFLAG:屈服度 > CFLAG:好感度 && !TALENT:MASTER:肉便器
				IF RAND:5 == 0
					;PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%に恋情を伝えようとしたが、訪問者を恐れてしまい果たせなかった。
				ENDIF
			ELSE
				IF 告白判定値 > 180 + TALENT:一線越えない * 20
					IF TALENT:MASTER:肉便器 && TARGET <= 人物_子悪魔
						PRINTFORMW %CALLNAME:TARGET% told to %CALLNAME:MASTER% that %HE_SHE(TARGET)% was quite pleased by %HIM_HER(MASTER)%.
					ELSE
						PRINTFORMW %CALLNAME:TARGET% conveyed %HIS_HER(TARGET)% love to %CALLNAME:MASTER%...
					ENDIF
					CALL KOJO_EVENT(13, TARGET)
					IF TALENT:MASTER:肉便器
						IF TARGET <= 人物_子悪魔
							PRINTFORMW %CALLNAME:MASTER% was told to serve even more actively by %CALLNAME:TARGET%, as %HIS_HER(TARGET)% cheeks were blushing a bit...
						ELSE
							PRINTFORMW %CALLNAME:MASTER% has accepted %CALLNAME:TARGET%'s confession even though %HE_SHE(MASTER)% wasn't really sure if a person like %HIM_HER(MASTER)% deserved such a good treatment...
						ENDIF
					ELSE
						PRINTFORMW %CALLNAME:MASTER% has accepted %CALLNAME:TARGET%'s confession while blushing profusely...
					ENDIF
					SETBIT CFLAG:既成事実,0
					
					PRINTL  
					PRINTFORM You and %CALLNAME:TARGET% are now a 
					SETCOLOR COLOR("PINK")
					PRINT Couple
					RESETCOLOR
					PRINTFORMW !
					PRINTL  
				ENDIF
			ENDIF
		ELSE
			TFLAG:COMABLE管理 = 1
		ENDIF
	ELSE
		TFLAG:COMABLE管理 = 0
	ENDIF
	;同じ場所にいるキャラクターを表示
	;0:ループ用キャラクター番号、1:同じ場所にいる人数
	同室人数 = 0
	PRINTFORM %"Currently here: "%
	;侵入者
	IF FLAG:侵入者の現在位置 == POS_NOW
		IF 同室人数
			PRINTFORM %", "%
		ENDIF
		PRINTFORM %"Koishi"%
		同室人数++
	ENDIF
	;侵入者
	IF FLAG:訪問者の現在位置 == POS_NOW
		IF 同室人数
			PRINTFORM %", "%
		ENDIF
		PRINTFORM %NTR_NAME(0)% 
		PRINT (Your Succumbing:
		CALL PRINT_アルファベット評価(MASTER,2)
		PRINTFORM  {CFLAG:MASTER:21})
		SIF FLAG:訪問者同行フラグ
			PRINT  is accompanying.
		同室人数++
	ENDIF
	;同じ場所にいる他のキャラクターを表示
	FOR LOOP_CHR,1,CHARANUM
		IF CFLAG:LOOP_CHR:現在位置 == POS_NOW
			IF 同室人数
				PRINT , 
			ENDIF
			IF CFLAG:LOOP_CHR:睡眠
				SETCOLOR 色設定_状態_睡眠中
				LOCALS = (sleeping)
			ELSEIF CFLAG:LOOP_CHR:同行
				SETCOLOR 色設定_状態_同行中
				LOCALS = (accompanying)
			ELSE
				LOCALS = 
			ENDIF
			SIF TARGET == LOOP_CHR
				SETCOLOR COLOR("aqua")
				LOCALS:1 = %CALLNAME:(LOOP_CHR)%
			PRINTBUTTON "["+LOCALS:1+"]"+LOCALS, 700 + LOOP_CHR
			RESETCOLOR
			同室人数++
		ENDIF
	NEXT
	PRINTL
	PRINTL
	IF 奴隷 > 0
		PRINTFORM %NAME:奴隷%
		PRINTBUTTON " [Abilities]",803
		
		PRINT  (Favorability:
		CALL PRINT_アルファベット評価(TARGET,1)
		PRINTFORM  {CFLAG:2}　
		PRINT Succumbing:
		CALL PRINT_アルファベット評価(TARGET,2)
		PRINTFORM  {CFLAG:21})
		IF CFLAG:奴隷:怒り
			SETCOLOR 色設定_状態_激おこ
			PRINTFORM %" (Angry)"%
			RESETCOLOR
		ELSE
			PRINTFORM %" Anger:"%
			SETCOLOR 色設定_状態_激おこ
			PRINTFORM %"！" * (BASE:怒り/200)%
			RESETCOLOR
		ENDIF
	ENDIF
RETURN 0

