@INFO_PrintRestroom(ARG:0)
	SELECTCASE REST_NeedRestroom(ARG:0)
		CASE 1
			SETCOLOR 色設定_状態_注意
			PRINTFORM %"[Urination]"%
		CASE 2
			SETCOLOR 色設定_状態_警告
			PRINTFORM %"[Urination]"%
	ENDSELECT
RETURN 0

;-------------------------------------------------------------------------------
;対象の衣服の状態を表示
;@param ARG キャラクターの番号
@INFO_PrintTargetCloth(奴隷)
	#DIM 奴隷
	#DIM 服番号
	#DIM 着衣数
	着衣数 = 0
	PRINTFORM Clothing (%CALLNAME:奴隷%): 
	IF !EQUIP:奴隷:衣服着用
		PRINTFORM %"[Naked]"%
	ELSE
		FOR 服番号,1,20
		
			SELECTCASE 服番号
				CASE GETNUM(EQUIP,"下半身下着１")
					IF (奴隷 != MASTER) && Unknown下着(奴隷, 服番号)
						PRINTFORM %"[？Underwear？]"%
						着衣数++
						CONTINUE
					ENDIF
				CASE GETNUM(EQUIP,"下半身下着２")
					IF (奴隷 != MASTER) && Unknown下着(奴隷, 服番号)
						PRINTFORM %""%
						着衣数++
						CONTINUE
					ENDIF
				CASEELSE
			ENDSELECT
			
			IF EQUIP:奴隷:服番号
				IF GETBIT(FLAG:匂いフェチ,1) 
					PRINTFORM %"["+IRUINAME_SMELL(EQUIP:奴隷:服番号)+CLOTHNAME(服番号,EQUIP:奴隷:服番号)+"]"%
				ELSE
					PRINTFORM %"["+CLOTHNAME(服番号,EQUIP:奴隷:服番号)+"]"%
				ENDIF
				着衣数++
			ENDIF
		NEXT
		IF 着衣数 == 0
			PRINTFORM %"[Naked]"%
		ENDIF
	ENDIF
	;最後に１回だけ改行
	PRINTL
RETURN 0

