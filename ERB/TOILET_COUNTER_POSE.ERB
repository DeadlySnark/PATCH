;69体勢
SELECTCASE TCVAR:ARG:20
	CASE 50
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	;フェラチオ
	CASE 51
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	;パイズリ
	CASE 53
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	;アナル愛撫
	CASE 55
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	;指挿入
	CASE 57
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	;クンニ強制
	CASE 70
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	;フェラチオ強制
	CASE 71
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	;アナル奉仕
	CASE 72
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	CASEELSE
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,0)
ENDSELECT

;引数1 対象のキャラ番号 2 行動順 3 シックスナイン移行への確率
@EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,ARG:2)
IF ARG:2 == 0
	TEQUIP:ARG:シックスナイン = 0
	RETURN 0
ENDIF
IF TEQUIP:ARG:シックスナイン
	TFLAG:60 ++
	RETURN 0
ENDIF
SIF TFLAG:60
	RETURN 0
IF RAND:(ARG:2) == 0
	TEQUIP:ARG:シックスナイン = 1
	TCVAR:ARG:27 = 1
ENDIF

