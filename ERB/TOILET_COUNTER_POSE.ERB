@EVENT_WC_COUNTER_POSE(ARG,ARG:1)
VARSET LOCAL
SIF TCVAR:ARG:30 > 1
	RETURN 0
SELECTCASE TCVAR:ARG:20
	;キス
	CASE 30
		TFLAG:60 ++
	;胸を揉ませる
	CASE 33
		TFLAG:61 ++
	;股間をまさぐる
	CASE 34
		TFLAG:62 ++
	;胸を揉む
	CASE 36
		TFLAG:65 ++
	;手淫
	CASE 50
		TFLAG:62 ++
	;フェラチオ
	CASE 51
		TFLAG:62 ++
	;素股
	CASE 52
		TFLAG:62 ++
	;パイズリ
	CASE 53
		TFLAG:62 ++
	;足コキ
	CASE 54
		TFLAG:62 ++
	;アナル愛撫
	CASE 55
		TFLAG:64 ++
	;アナル舐め
	CASE 56
		TFLAG:64 ++
	;指挿入
	CASE 57
		TFLAG:63 ++
	;胸愛撫
	CASE 58
		TFLAG:65 ++
	;乳首吸い
	CASE 59
		TFLAG:65 ++
	;ディープキス
	CASE 60
