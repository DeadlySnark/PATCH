	;挿入される
	CASE 80
		TFLAG:63 ++
	;騎乗位アナル
	CASE 81
		TFLAG:64 ++
	;後背位アナル
	CASE 82
		TFLAG:64 ++
	;背面座位アナル
	CASE 83
