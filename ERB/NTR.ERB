@NTR_V_EXP_PLUS(奴隷)
	#DIM 奴隷
	IF ABL:奴隷:Ｖ感覚 >= 2
		IF RAND:3 == 0
			EXP:奴隷:Ｖ経験 ++
			EXP:奴隷:Ｖ絶頂経験 ++
