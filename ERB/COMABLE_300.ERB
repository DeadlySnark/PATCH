;押し倒される
@COM_ABLE349
;押し倒す実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(349)
	RETURN RESULT

SIF TARGET < 1
	RETURN 0
;睡眠中
SIF CFLAG:睡眠
	RETURN 0
;気力0
SIF BASE:MASTER:気力 <= 0
	RETURN 0

;相手が１人だけで、公衆便所で、その相手の貞操帯の鍵を持っている場合は実行可能
;鍵なしで公衆便所は押し倒せない＆複数人の場合は全員が押し倒せる状態じゃないとダメ
;上記の理由により、１人の時に限定して判定して良いはず
IF GET_TARGETNUM() == 1 && TALENT:(TARGET:0):公衆便所 && FLAG:貞操帯鍵購入フラグ == TARGET:0
	RETURN 1
ENDIF

SIF TALENT:MASTER:肉便器
	RETURN 1
	
;ターゲットが２人以上いる
IF GET_TARGETNUM() > 1
	;誰かが眠っている
	IF GET_TARGETSLEEP()
		;起きている人数による条件変化
		IF GET_TARGETWAKEUP() >= 2 && TCVAR:MASTER:馴れ合い強度 < 3
			RETURN 0
		ELSEIF GET_TARGETWAKEUP() == 1 && TCVAR:MASTER:馴れ合い強度 < 2
			RETURN 0
		ENDIF
	;全員起きている
	;まとめて押し倒す場合には、全体の馴れ合い強度が３以上必要
	ELSEIF TCVAR:MASTER:馴れ合い強度 < 3
		RETURN 0
	ENDIF
ENDIF

FOR LOCAL,1,CHARANUM
	SIF TARGET:LOCAL <= 0
		CONTINUE
	;睡眠中は回避
	IF !CFLAG:(TARGET:LOCAL):睡眠
		;相手に合意がない場合はダメ
		IF !GETBIT(CFLAG:(TARGET:LOCAL):既成事実,0)
			RETURN 0
		ENDIF
	ENDIF
NEXT
SIF CFLAG:317 == 2
	RETURN 0
SIF CFLAG:317 == 3
	RETURN 0
;訪問者と住人１人同席、かつ主人が空気
SIF FLAG:訪問者の現在位置 == CFLAG:MASTER:現在位置 && GET_N_WITH_VISITER(TARGET) == 2 && IS_AIR_MASTER(TARGET)
	RETURN 0
RETURN 1

;押し倒す
@COM_ABLE350
;押し倒す実行判定
IF !TFLAG:100
	RETURN 0
ENDIF
;一括管理
IF GLOBAL_COMABLE(350)
	RETURN RESULT
ENDIF
;ターゲットが睡眠中はダメ
SIF CFLAG:睡眠
	RETURN 0
;あなたの気力が0だとダメ
SIF BASE:MASTER:気力 <= 0
	RETURN 0
;相手が１人だけで、公衆便所で、その相手の貞操帯の鍵を持っている場合は実行可能
;鍵なしで公衆便所は押し倒せない＆複数人の場合は全員が押し倒せる状態じゃないとダメ
;上記の理由により、１人の時に限定して判定して良いはず
IF GET_TARGETNUM() == 1 && TALENT:(TARGET:0):公衆便所 && FLAG:貞操帯鍵購入フラグ == TARGET:0
	RETURN 1
ENDIF
;馴れ合い強度2以上必要（弱味を握られている場合3）;;肉便器だと更に+1
SIF TCVAR:MASTER:馴れ合い強度 < 2 + (CFLAG:TARGET:MASTERの弱味>0) + TALENT:MASTER:肉便器
	RETURN 0
	
;ターゲットが２人以上いる
IF GET_TARGETNUM() > 1
	;誰かが眠っている
	IF GET_TARGETSLEEP()
		;起きている人数による条件変化
		IF GET_TARGETWAKEUP() >= 2 && TCVAR:MASTER:馴れ合い強度 < 3
			RETURN 0
		ELSEIF GET_TARGETWAKEUP() == 1 && TCVAR:MASTER:馴れ合い強度 < 2
			RETURN 0
		ENDIF
	;全員起きている
	;まとめて押し倒す場合には、全体の馴れ合い強度が３以上必要
	ELSEIF TCVAR:MASTER:馴れ合い強度 < 3
		RETURN 0
	ENDIF
ENDIF

FOR LOCAL,1,CHARANUM
	SIF TARGET:LOCAL <= 0
		CONTINUE
	;睡眠中は回避
	IF !CFLAG:(TARGET:LOCAL):睡眠
		;相手に合意がない場合はダメ
		IF !GETBIT(CFLAG:(TARGET:LOCAL):既成事実,0)
			RETURN 0
		ENDIF
	ENDIF
NEXT

;訪問者と住人１人同席、かつ主人が空気
SIF FLAG:訪問者の現在位置 == CFLAG:MASTER:現在位置 && GET_N_WITH_VISITER(TARGET) == 2 && IS_AIR_MASTER(TARGET)
	RETURN 0
RETURN 1

