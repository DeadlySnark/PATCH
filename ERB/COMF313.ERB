;胸愛撫
@COM313

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_胸元)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 5
DOWNBASE:気力 += 50
SOURCE:快Ｂ = 120
SOURCE:歓楽 = 100
SOURCE:情愛 = 150
SOURCE:欲情 = 170
SOURCE:露出 = 70
SOURCE:反感 = 350

SOURCE:受動 = 120 + 240 * ABL:従順

;着衣補正
IF TEQUIP:1 > 1
	TIMES SOURCE:快Ｂ , 0.80
	TIMES SOURCE:欲情 , 0.80
	TIMES SOURCE:露出 , 0.80
ENDIF
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 20
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ⇔調教者の指の汚れが移動
STAIN:Ｂ |= STAIN:PLAYER:手
STAIN:PLAYER:手 |= STAIN:Ｂ

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 3
ENDIF
TIME += 10
RETURN 1

