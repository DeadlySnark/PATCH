;-------------------------------------------------
;膣鏡
;-------------------------------------------------
@COM149

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_太股)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:気力 += 30
DOWNBASE:気力 += 10

SOURCE:苦痛 = 200
SOURCE:恐怖 = 350
SOURCE:逸脱 = 50
SOURCE:反感 = 300


IF TEQUIP:膣鏡 == 0 && PALAM:潤滑 < PALAMLV:3
	SIF CFLAG:現在位置 != 5 && ITEM:ローション
		ITEM:ローション -= 1
	SOURCE:液体 = 5000
ENDIF
;着脱の処理
TEQUIP:膣鏡 = !TEQUIP:膣鏡

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;苦痛のパラメータによる苦痛のソース増加は先に処理しておく
SOURCE:苦痛 = SOURCE:苦痛 * (10 + 5 * MAX(GETPALAMLV(PALAM:苦痛,5) - 1, 0)) / 10
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 2
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 2
ENDIF

RETURN 1
